<h1>Listing team_listings</h1>

<table>
  <thead>
    <tr>
      <th>Team</th>
      <th>Event</th>
      <th>Lane number</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @team_listings.each do |team_listing| %>
      <tr>
        <td><%= team_listing.team %></td>
        <td><%= team_listing.event %></td>
        <td><%= team_listing.lane_number %></td>
        <td><%= link_to 'Show', team_listing %></td>
        <td><%= link_to 'Edit', edit_team_listing_path(team_listing) %></td>
        <td><%= link_to 'Destroy', team_listing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Team listing', new_team_listing_path %>
