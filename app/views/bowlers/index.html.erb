<h1>Listing bowlers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Team</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @bowlers.each do |bowler| %>
      <tr>
        <td><%= bowler.name %></td>
        <td><%= bowler.team %></td>
        <td><%= link_to 'Show', bowler %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
